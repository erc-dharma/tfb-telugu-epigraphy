         <div type="edition" xml:lang="tel-Latn">
            <p>
             <lb n="1"/>śrī mahēndraN manaravi māṟurāpi<gap reason="undefined" unit="character" quantity="2" precision="low"/>
             <lb n="2"/>maganṟu pudali Ēḷuvānṟu kāpibōḷamutturāju
             <lb n="3"/>acca<supplied reason="omitted">r</supplied>vvuganu arivajamunṟu anvānṟu
             <lb n="4"/>dantiyamma maṁgutōḻaN poḍuci paḍiyeN
            </p>
         </div>
         <div type="apparatus">
               <listApp>
                  <app loc="1">
                     <lem>°ndraN manaravi māṟurāpi</lem>
                     <rdg source="bib:RamayyaPantulu1948_01">
                           °ndrasamāna<unclear>ra</unclear>vi māṟu<unclear>kā</unclear>pi</rdg>
                  </app>
                  <app loc="2">
                     <lem>Ēḷuvānṟu kāpibōḷamutturāju</lem>
                     <rdg source="bib:RamayyaPantulu1948_01">
                          Ē<unclear>ḷu</unclear>vānṟu kāpi</rdg>      
                  </app>
                  <app loc="3">
                     <lem>acca<supplied reason="omitted">r</supplied>vvuganu arivajamunṟu anvānṟu</lem>
                     <rdg source="bib:RamayyaPantulu1948_01">
                          avvanṟu<unclear>la</unclear>nu arivarajamunṟu anvanṟu</rdg>
                  </app>
                  <app loc="4">
                     <lem>dantiyamma maṁgutōḻaN poḍuci paḍiyeN</lem>
                     <rdg source="bib:RamayyaPantulu1948_01">
                          <unclear>danti</unclear>yamma veguṇa<gap reason="undefined" unit="character" quantity="2" precision="low"/> poḍuci paḍiye</rdg>
                  </app>
               </listApp>         
         </div>
